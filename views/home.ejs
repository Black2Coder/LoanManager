<%- include('./base.ejs') %>






    <div class="container mt-md-5 " style="width: 50rem;">
        <form action="/home" method="GET">
            <div class="form-group">
                <label for="name">Full Name</label>
                <input type="text" class="form-control" name="name" placeholder="Full Name As per Document" required>
            </div>
            <div class="form-group">
                <label for="email">Email</label>
                <input type="email" class="form-control" name="email" placeholder="Email">
            </div>
            <div class="form-group">
                <label for="phone">Phone Number</label>
                <input type="number" class="form-control" name="phone" placeholder="Phone" required>
            </div>
            <div class="form-group">
                <label for="pancard">Amount of Loan</label>
                <input type="number" class="form-control" name="amount" placeholder="Amount of Loan" required>

            </div>

            <div class="form-group">
                <label for="adhar">Adhaar</label>
                <input type="number" class="form-control" name="adhar" placeholder="Adhar" required>
            </div>

            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
    <!-- Optional JavaScript -->
    <script>
        if (window.history.replaceState) {
            window.history.replaceState(null, null, window.location.href);
        }
        formSubmit.addEventListener('click', (event) => {
